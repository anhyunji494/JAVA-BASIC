package day4;

public class Ex0099 {

	public static void main(String[] args) {
		// 21 ~ 57 출력
		for (int i = 21; i <= 57; i++) {
			System.out.print(i + " ");
		}
		System.out.println();

		// 21~53 중 홀수만 출력
		for (int i = 21; i <= 53; i++) {
			System.out.print( i * 2 + " ");
		}	

		System.out.println();

		// 22~57 중 홀수 출력

		System.out.println();

		// 1~100까지 숫자 중 3의 배수를 가로로 출력

		// 1) 1부터 100까지 가로로 출력

		System.out.println();

		// 2) 그 중 3의 배수일 때만 출력

		System.out.println();

		// 57부터 21까지 거꾸로 출력

		System.out.println();

		// 가독성을 위해 아래를 선호함 : 두 수의 합을 이용 y = -x + 78 함수

		System.out.println();
	}
}
