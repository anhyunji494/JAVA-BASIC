package festival;

import java.util.Scanner;

public class SecondQ07 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("행 개수 : ");
		int num = sc.nextInt();

		for (int j = 0; j < num; j++) {
			for (int i = num; i > 0; i--) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
