package day4;

public class Ex0099 {

	public static void main(String[] args) {
		// 21 ~ 57 출력
		for (int i = 21; i <= 57; i++) {
			System.out.print(i + " ");
		}
		System.out.println();

		// 21~57 중 홀수만 출력
		// 21, 23, 25 ... 51, 53, 55, 57
		for (int i = 21; i <= 57; i++) {
			if(i % 2 == 1) {
				System.out.print(i + " ");
			}
		}	

		System.out.println();

		// 1~100까지 숫자 중 3의 배수를 가로로 출력
		for(int i = 1; i <= 100; i++) {
			if(i % 3 == 0) {
				System.out.print(i + " ");
			}
		}
		

		// 1) 1부터 100까지 가로로 출력

		System.out.println();

		// 2) 그 중 3의 배수일 때만 출력

		System.out.println();

		// 57부터 21까지 거꾸로 출력

		System.out.println();

		// 가독성을 위해 아래를 선호함 : 두 수의 합을 이용 y = -x + 78 함수

		System.out.println();
	}
}
