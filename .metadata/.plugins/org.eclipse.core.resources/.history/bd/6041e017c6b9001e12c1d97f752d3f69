package festival;

import java.util.Scanner;

public class SecondQ27 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("==== 채점하기 ====");
		String answer = sc.next();
		int cnt = 0;

		switch (answer) {
		case "o":
			cnt++;
			break;

		case "x":
			break;
		}
		System.out.println(cnt);
	}

}
