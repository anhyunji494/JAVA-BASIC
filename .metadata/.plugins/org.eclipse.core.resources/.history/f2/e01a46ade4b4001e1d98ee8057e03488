package solve;

import java.util.Scanner;

public class B01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// A입력
		System.out.print("A 입력 >> ");
		int a = sc.nextInt();

		// B입력
		System.out.print("B 입력 >>");
		int b = sc.nextInt();

		System.out.print("결과 >> " + (a - b));
		
		if(a==0 && b==0) {
			System.out.println(" ");
		}

	}

}
